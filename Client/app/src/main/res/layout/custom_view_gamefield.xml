        app:layout_constraintBottom_toBottomOf="@id/field_square"
        app:layout_constraintEnd_toEndOf="@id/field_square"
        app:layout_constraintTop_toTopOf="@id/guideline_bottom"
        android:onClick="@{() -> viewModel.onImageViewClick(startField)}"
        app:srcCompat="@drawable/start" />

